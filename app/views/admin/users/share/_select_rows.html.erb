<div class='col-xs-1' style="margin-left:-0.9%" >
  <%= form_tag('/agency_admin/tenants', method: :get, id: "changePage") do %>
    <%= select_tag 'per_page',
      options_for_select([["10","10"], ["25","25"], ["50","50"], ["100","100"]], selected: params[:per_page]),
      {onchange: "$('#changePage').submit();" }
    %>
  <% end %>
</div>
